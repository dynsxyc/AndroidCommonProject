<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/common_white_press" />
            <corners android:radius="@dimen/dp_10" />
            <size android:width="@dimen/dp_20" android:height="@dimen/dp_20" />
        </shape>
    </item>
    <item>
        <shape android:checked="false" android:shape="oval">
            <solid android:color="@color/common_white_4d" />
            <corners android:radius="@dimen/dp_10" />
            <size android:width="@dimen/dp_20" android:height="@dimen/dp_20" />
        </shape>
    </item>
</selector>